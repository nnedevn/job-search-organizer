<div class="wrapper">
  <div id="content">
    <!-- loading image -->
    <div>
      <div class="">
        <% jobs.forEach(function(job){ %>
          <div class="single-post">
            <a href="<%=job.jobUrl%>">
              <h4>
                <%= job.title %>
              </h4>
            </a>
            <p class="company-info">
              <%= job.companyName %> in
                <%= job.companyLocation %>
            </p>
            <p>
              <%= job.summary %>
            </p>
            <p>Found on:
              <span class="origin-site">Indeed.com</span>
            </p>
            <% if(job.appliedFor) {%>
              <button type="submit" data-jobId="<%= job.id %>" id="" class="btn btn-warning addToAppliedJob" disabled>Applied</button>
              <%} else {%>
                <button type="submit" data-jobId="<%= job.id %>" id="" class="btn btn-success addToAppliedJob">Add to Applied</button>
                <%}%>
            <button data-jobId="<%= job.id %>" id="" class="btn btn-danger deleteJob">Delete from Saved</button>
          </div>
          <% }); %>
      </div>
    </div>