<div class="wrapper">
  <div id="content">
    <div>
      <div class="">
        <% jobs.forEach(function(job){ %>
          <div class="single-post">
            <a href="<%=job.jobUrl%>">
              <h4>
                <%= job.title %>
              </h4>
            </a>
            <p class="company-info">
              <%= job.companyName %> in
                <%= job.companyLocation %>
            </p>
            <p>
              <%= job.summary %>
            </p>
            <p>Found on:
              <span class="origin-site">Indeed.com</span>
            </p>
            <button data-jobId="<%= job.id %>" id="" class="btn btn-warning removeFromAppliedJob">Remove from Applied</button>
          </div>
          <% }); %>
      </div>
    </div>